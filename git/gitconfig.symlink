[user]
  name = Trace Helms
  email = traceh@izea.com

[credential]
  helper = osxkeychain

[core]
  excludesfile = ~/.gitignore

[mergetool]
  keepBackup = false

[color]
  ui = true

[apply]
  whitespace = nowarn

[alias]
  co = checkout
  po = pull origin
  pso = push origin
  cm = commit -m 
  br = branch
  st = status
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
